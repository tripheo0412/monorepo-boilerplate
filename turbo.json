{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "storybook-static/**",
        "build/**",
        "out/**",
        "lib/**",
        ".next/**"
      ]
    },
    "build-libs": {
      "outputs": [
        "dist/**"
      ]
    },
    "build-type": {
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "type-check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "prettier": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}